[flake8]
select = ANN,A50,B,B9,BLK,C,D,DAR,E,F,I,S,W
docstring-convention = google
max-complexity = 15
ignore = ANN101,ANN401,B009,E203,E501,W503,S113,S402,S321
max-line-length = 88
per-file-ignores = __init__.py: F401,tests/*:S101,image_service.py: F401
application-import-names = tests
import-order-style = google
